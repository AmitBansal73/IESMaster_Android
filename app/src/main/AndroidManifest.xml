<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iesmaster">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ies"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ies"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TopicActivity"
            android:parentActivityName=".UnitActivity" />
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UnitActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".YearsActivity"
            android:parentActivityName=".HomeActivity" />

        <activity android:name=".HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".WelcomeActivity">
        </activity>

        <activity android:name=".PaymentActivity" />

        <activity
            android:name=".Questions.QuestionsActivity"
            android:parentActivityName=".UnitActivity" />

        <activity android:name=".SubjectActivity" />

        <activity android:name=".TestPaperActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".AcademicProfileActivity"
            android:parentActivityName=".HomeActivity" />

        <activity android:name=".MainActivity" />
    </application>

</manifest>